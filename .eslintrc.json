{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "ignorePatterns": [
    "output/**",
    "prisma/seed.ts",
    "public/sw.js",
    "public/workbox-*.js",
    ".next/**",
    "node_modules/**"
  ],
  "rules": {
    "@next/next/no-img-element": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "react-hooks/exhaustive-deps": "warn"
  }
}
